#: C/xreader.xml:720(keycap) C/xreader.xml:728(keycap)
msgid "Return"
msgstr "Eingabetaste"

#: C/xreader.xml:722(para)
msgid "Scroll forward"
msgstr "Mausrad vorwärts"

#: C/xreader.xml:730(para)
msgid "Scroll backward"
msgstr "Mausrad rückwärts"

#: C/xreader.xml:735(para)
msgid "Go a bunch of pages up"
msgstr "Mehrere Seiten nach oben gehen"

#: C/xreader.xml:739(para)
msgid "Go a bunch of pages down"
msgstr "Mehrere Seiten nach unten gehen"

#: C/xreader.xml:742(keycap)
msgid "Home"
msgstr "Pos1"

#: C/xreader.xml:743(para)
msgid "Go to the first page"
msgstr "Zur ersten Seite gehen"

#: C/xreader.xml:746(keycap)
msgid "End"
msgstr "Ende"

#: C/xreader.xml:747(para)
msgid "Go to the last page"
msgstr "Zur letzten Seite gehen"

#: C/xreader.xml:750(keycap)
msgid "L"
msgstr "L"

#: C/xreader.xml:751(para)
