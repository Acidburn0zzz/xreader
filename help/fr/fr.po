#: C/xreader.xml:722(para)
msgid "Scroll forward"
msgstr "Défile vers le bas"

#: C/xreader.xml:730(para)
msgid "Scroll backward"
msgstr "Défile vers le haut"

#: C/xreader.xml:735(para)
msgid "Go a bunch of pages up"
msgstr "Recule de quelques pages"

#: C/xreader.xml:739(para)
msgid "Go a bunch of pages down"
msgstr "Avance de quelques pages"

#: C/xreader.xml:742(keycap)
msgid "Home"
msgstr "Origine"

#: C/xreader.xml:743(para)
msgid "Go to the first page"
msgstr "Va à la première page"

#: C/xreader.xml:746(keycap)
msgid "End"
msgstr "Fin"

#: C/xreader.xml:747(para)
msgid "Go to the last page"
msgstr "Va à la dernière page"

#: C/xreader.xml:750(keycap)
msgid "L"
msgstr "L"

#: C/xreader.xml:751(para)
msgid "Go to page by number or label"
msgstr "Va à la page selon son numéro ou son étiquette"

#: C/xreader.xml:754(keycap)
msgid "F1"
msgstr "F1"

#: C/xreader.xml:755(para)
msgid "Help"
msgstr "Aide"

#: C/xreader.xml:758(keycap)
msgid "F5"
msgstr "F5"

#: C/xreader.xml:759(para)
msgid "Run document as presentation"
msgstr "Lance le document en mode présentation"

