msgstr "Scorre in avanti"

#: C/xreader.xml:727(para)
msgid "Scroll backward"
msgstr "Scorre indietro"

#: C/xreader.xml:732(para)
msgid "Go a bunch of pages up"
msgstr "Va un po' di pagine indietro"

#: C/xreader.xml:736(para)
msgid "Go a bunch of pages down"
msgstr "Va un po' di pagine in avanti"

#: C/xreader.xml:739(keycap)
msgid "Home"
msgstr "Home"

#: C/xreader.xml:740(para)
msgid "Go to the first page"
msgstr "Va alla prima pagina"

#: C/xreader.xml:743(keycap)
msgid "End"
msgstr "Fine"

#: C/xreader.xml:744(para)
msgid "Go to the last page"
msgstr "Va all'ultima pagina"

#: C/xreader.xml:747(keycap)
msgid "L"
msgstr "L"

#: C/xreader.xml:748(para)
msgid "Go to page by number or label"
msgstr "Va alla pagina in base al numero o all'etichetta"

#: C/xreader.xml:751(keycap)
msgid "F1"
msgstr "F1"

#: C/xreader.xml:752(para)
msgid "Help"
msgstr "Aiuto"

#: C/xreader.xml:755(keycap)
msgid "F5"
msgstr "F5"

#: C/xreader.xml:756(para)
msgid "Run document as presentation"
msgstr "Avvia la presentazione"

